    { "when": { "south": "true"  }, "apply": { "model": "cctransfer:tube_basic_side", "x": 0,   "y": 180, "uvlock": true } },
    { "when": { "west":  "true"  }, "apply": { "model": "cctransfer:tube_basic_side", "x": 0,   "y": 270, "uvlock": true } },
    { "when": { "east":  "true"  }, "apply": { "model": "cctransfer:tube_basic_side", "x": 0,   "y": 90,  "uvlock": true } }
